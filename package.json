{
  "name": "dombars",
  "version": "0.0.0",
  "description": "An extension of Handlebars that generates DOM objects instead of strings.",
  "main": "./lib/dombars.js",
  "scripts": {
    "build": "browserify ./lib/dombars.js -s DOMBars -o dombars.bundle.js",
    "test": "npm run-script build && ./node_modules/.bin/mocha-phantomjs ./test/index.html"
  },
  "repository": "https://github.com/blakeembrey/dombars",
